  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/api/main.go",
      "env": {
        "WORKFORCE_DB_USER": "workforce_user",
        "WORKFORCE_DB_PASS": "workforce_pass",
        "WORKFORCE_DB_NAME": "postgres",
        "WORKFORCE_DB_HOST": "localhost",
        "WORKFORCE_DB_SSL_MODE": "disable",
        "DB_POOL_MAX_CONNS": "10",
        "DB_POOL_MAX_CONN_IDLE_TIME": "30m",
        "DB_POOL_MIN_CONNS": "5"
      },
      "args": []
    }
  ]
